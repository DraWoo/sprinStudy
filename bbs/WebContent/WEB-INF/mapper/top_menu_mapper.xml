<?xml version = '1.0' encoding='UTF-8'?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="topmenu">
	<select id="get_topmenu_list" resultType="com.web.bbs.beans.TbInfoTable">
		<![CDATA[
				select tb_info_idx
					 , tb_info_name
				from tb_info_table
				order by tb_info_idx	 	
		]]>
	</select>
</mapper>						